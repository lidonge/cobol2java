{{%tableCall=null;""}}
{{#delegate}}
    {{%
        subscripts=null;
    ""}}
    {{#subscripts}}
        {{#subscriptValueStmt}}
            {{>call.subscript}}
            {{%
                subscripts = subscripts == null?subscript:subscripts+","+subscript;
            ""}}
        {{/subscriptValueStmt}}
    {{/subscripts}}
    {{%
        callType = obj_getVar("callType");
        isTable = callType == "TABLE_CALL" ? "isTable":null;
    ""}}
    {{#$isTable}}
        {{>call.qlfName}}
        {{%
            //FIXME the bug of (1,1)
            dimString = subscripts == null ?dim_udfCall(obj_getVar("ctx.text")):subscripts+"";
            tableCall=name_qlfNameWithDim(qlfName, dimString);
        ""}}
    {{/isTable}}
{{/delegate}}